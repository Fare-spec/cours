<script language="javascript">

	if (document.getElementById('username')) {
		document.getElementById('username').focus();
	}

	function getCookie(cname) {
		var cname;
		let name = cname + "=";
		let decodedCookie = decodeURIComponent(document.cookie);
		let ca = decodedCookie.split(';');
		for(let i = 0; i <ca.length; i++) {
			let c = ca[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}

	function maskIt(id) {
		var id;
		document.cookie = "Moodle_" + id + "_display=false; SameSite=Strict; Secure; path =/;";
	}

	if (getCookie("Moodle_infobanner3_display") === "false") {
		let id = "themeboostunioninfobanner3";
		if (document.getElementById(id)) {
			document.getElementById(id).style.display = 'none';
		}
	}

	if (getCookie("Moodle_infobanner4_display") === "false") {
		let id = "themeboostunioninfobanner4";
		if (document.getElementById(id)) {
			document.getElementById(id).style.display = 'none';
		}
	}

</script>
